public class Ej19 {

	public static void main(String[] args) {
		Integer cont = 0;
		for(int i = 1; i <= 9; i++) { //este es el que baja
			 System.out.println();
			 
			for(int j = 1; j <= i; j++) { // este es el que va a la derecha
				System.out.print(j + " ");
			}
			
		}
		
	}

}
