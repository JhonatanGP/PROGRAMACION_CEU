package tema04.EjBasicos01.Ej09;

public class Ej09 {

	public static void main(String[] args) {
		Integer result1 = ArraysUtils.getCalcular(ArraysUtils.SUMAR, 8, 4);
		Integer result2 = ArraysUtils.getCalcular(ArraysUtils.RESTAR, 8, 4);
		Integer result3 = ArraysUtils.getCalcular(ArraysUtils.MULTIPLICAR, 8, 4);
		Integer result4 = ArraysUtils.getCalcular(ArraysUtils.DIVIDIR, 8, 4);
		System.out.println(result1);
		System.out.println(result2);
		System.out.println(result3);
		System.out.println(result4);
		
	}

}
