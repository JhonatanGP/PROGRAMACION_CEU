package tema04.EjRepaso.Ej4;

import java.time.LocalDate;

public class Fisico extends Libro{
	
	private int alto;
	private int ancho;
	private int peso;
	
	public Fisico(String titulo, String autor, LocalDate fechaEdicion, Integer pagina) {
		super(titulo, autor, fechaEdicion, pagina);
	}
	

	public int getAlto() {
		return alto;
	}

	public void setAlto(int alto) {
		this.alto = alto;
	}

	public int getAncho() {
		return ancho;
	}

	public void setAncho(int ancho) {
		this.ancho = ancho;
	}

	public int getPeso() {
		return peso;
	}

	public void setPeso(int peso) {
		this.peso = peso;
	}
	
	
	

}
