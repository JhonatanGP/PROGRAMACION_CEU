package Tema02.ej03Pr√°cticosRepaso;

public class Ej200 {

	public static void main(String[] args) {

		// Integer t = 5;

		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 5; j++) {
				if (i == j) {
					System.out.println("* ");
				}else {
					System.out.println("- ");
				}

			}
		}
	}
}
