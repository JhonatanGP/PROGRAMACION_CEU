package tema03.ejRepaso;

public class Ej02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Integer[] lista = new Integer[] { 7, 9, 8, 4, 0, 6, 3, 4, 1, 7, 3, 1, 9, 10 };
		Integer suma = 0;
		for (int i = 0; i < lista.length; i++) {
			if(lista[i] < 5) {
				suma = suma + i;
			}
		}
		System.out.print("Nota media de aprobados: " + suma/2);// si pongo lista[i] me imprime las notas y si pongo cont
																// me imprime el numero de suspensos

	}

}
