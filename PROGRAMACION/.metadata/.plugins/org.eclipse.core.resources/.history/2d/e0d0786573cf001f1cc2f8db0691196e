package tema04.EjBasicos01.Ej08;

import java.util.Scanner;

import tema04.EjBasicos01.Ej06.Calculadora;

public class Ej08 {

	public static void main(String[] args) {
		Integer result1 = Calculadora.getCalcular(Calculadora.SUMAR, 8, 4);
		Integer result2 = Calculadora.getCalcular(Calculadora.RESTAR, 8, 4);
		Integer result3 = Calculadora.getCalcular(Calculadora.MULTIPLICAR, 8, 4);
		Integer result4 = Calculadora.getCalcular(Calculadora.DIVIDIR, 8, 4);
		System.out.println(result1);
		System.out.println(result2);
		System.out.println(result3);
		System.out.println(result4);
		
	}

}
