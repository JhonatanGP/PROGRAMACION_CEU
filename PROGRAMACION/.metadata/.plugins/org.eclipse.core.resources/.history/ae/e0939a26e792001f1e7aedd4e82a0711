package ej02Prácticos;

import java.util.Scanner;

public class Ej06 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		Integer cont = 1;
		
		do {
			System.out.println("Mi nombre es Jhonatan." + " Línea " + cont + ".");
			cont ++;

		}while(cont <=5);
					
		sc.close();
		
	}

}
