package tema04.EjBasicos01.Repaso;

public class Caballo {
	private String nombre;
	private Integer carrerasGanadas;
	private Integer edad;
	private String color;
	
	public void imprimeCarrerasGanadas() {
		System.out.println("El nÃºmero de carreras ganadas es " + this.carrerasGanadas);
	}
	
	public String getNombre() {
		return nombre;
	}
	public void setNombre(String nombre) {
		this.nombre = nombre;
	}
	public Integer getCarrerasGanadas() {
		return carrerasGanadas;
	}
	public void setCarrerasGanadas(Integer carrerasGanadas) {
		this.carrerasGanadas = carrerasGanadas;
	}
	public Integer getEdad() {
		return edad;
	}
	public void setEdad(Integer edad) {
		this.edad = edad;
	}
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
	
	

}
