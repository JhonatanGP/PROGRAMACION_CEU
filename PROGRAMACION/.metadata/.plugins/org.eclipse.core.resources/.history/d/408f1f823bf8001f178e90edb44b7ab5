package tema04.EjRepaso.Ej01;

import java.time.LocalDate;

public class Smartphone {
	private String marca;
	private String modelo;
	private String imei;
	private Double precio;
	private LocalDate fechaFabricacion;
	private Boolean activo;
	
	
	public Smartphone() {
		super();
		}
	
	public Smartphone(String marca, String modelo, String imei, Double precio, LocalDate fechaFabricacion,
			Boolean activo) {
		super();
		this.marca = marca;
		this.modelo = modelo;
		this.imei = imei;
		this.precio = precio;
		this.fechaFabricacion = fechaFabricacion;
		this.activo = activo;
	}
	public Smartphone(String marca, String modelo, String imei, Double precio, LocalDate fechaFabricacion,
			Boolean activo) {
		super();
		this.marca = marca;
		this.modelo = modelo;
		this.imei = imei;
		this.precio = precio;
		this.fechaFabricacion = fechaFabricacion;
		this.activo = activo;
	}
	
	

	



	

}
